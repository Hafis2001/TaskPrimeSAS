{
  "expo": {
    "name": "TaskPrimeSAS",
    "slug": "TaskPrimeSAS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/taskprime-icon.jpg",
    "scheme": "taskprime",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/taskprime-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#ffffff",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/taskprime-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hafis2001.TaskPrimeSAS",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/images/taskprime-icon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/taskprime-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
      
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7c88ba1f-670c-4ab8-8c79-277250f17498"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
